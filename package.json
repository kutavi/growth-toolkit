{
  "name": "growth-toolkit",
  "private": true,
  "description": "A toolkit with exercises and activities to help you on your personal development and growth.",
  "version": "0.0.0",
  "author": "Alexandra Tseniklidou",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --mode development --open",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,scss}\"",
    "lint": "npm run format && eslint \"src/**/*.{ts,tsx}\" --fix",
    "compress-images": "node scripts/compress-images.js",
    "analyze": "webpack --mode production --profile --json > stats.json",
    "analyze:visualize": "webpack-bundle-analyzer stats.json dist"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "classnames": "^2.5.1",
    "html2canvas": "^1.4.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-responsive-modal": "^7.1.0",
    "react-router-dom": "^7.9.5",
    "react-select": "^5.10.2",
    "sass": "^1.93.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.38.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "file-loader": "^6.2.0",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.4",
    "prettier": "^3.6.2",
    "sass-loader": "^16.0.6",
    "sharp": "^0.34.4",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "keywords": [
    "react",
    "personal-development",
    "growth-toolkit"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kutavi/growth-toolkit"
  },
  "bugs": {
    "url": "https://github.com/kutavi/growth-toolkit/issues"
  }
